{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "trailing-comma": [true, {
      "singleline": "never",
      "multiline": {
        "objects": "never",
        "arrays": "never",
        "functions": "never",
        "typeLiterals": "never"
      }
    }]
  }
}
